{"ast":null,"code":"import io from 'socket.io-client';\nimport fonctionnaliteesAttaque from \"@/components/plateauAttaque/fonctionnaliteesAttaque.vue\";\nimport caseAttaquant_attaque from \"@/components/plateauAttaque/Case_attaquant/CaseAttaquant_attaque.vue\";\nlet carteEnMain = [];\nlet a = caseAttaquant_attaque.data().readers;\nexport default {\n  data() {\n    return {\n      socket: null,\n      images_compteur: [require('@/img/compteur/compteur_1.png'), require('@/img/compteur/compteur_2.png'), require('@/img/compteur/compteur_3.png'), require('@/img/compteur/compteur_4.png'), require('@/img/compteur/compteur_5.png')],\n      visibility: [true, true, true, true, true]\n    };\n  },\n  mounted() {\n    this.socket = io('http://localhost:3000');\n    this.socket.on('rfidData', data => {\n      const {\n        readerID\n      } = data;\n\n      // Vérifier si le readerID est 1\n      if (readerID === '1)') {\n        let deckDefense = fonctionnaliteesAttaque.methods.genererDeckDefense();\n        console.log(\"Deck generer\");\n        console.log(deckDefense);\n        fonctionnaliteesAttaque.methods.DebutTour(deckDefense, carteEnMain);\n        let reader = a.find(reader => reader.id === 4);\n        console.log(\"ID\");\n        console.log(reader);\n        fonctionnaliteesAttaque.methods.poserCarte(carteEnMain, reader);\n        this.updateVisibility();\n      }\n    });\n  },\n  methods: {\n    updateVisibility() {\n      for (let i = this.visibility.length - 1; i >= 0; i--) {\n        if (this.visibility[i]) {\n          this.visibility[i] = false;\n          break;\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["io","fonctionnaliteesAttaque","caseAttaquant_attaque","carteEnMain","a","data","readers","socket","images_compteur","require","visibility","mounted","on","readerID","deckDefense","methods","genererDeckDefense","console","log","DebutTour","reader","find","id","poserCarte","updateVisibility","i","length"],"sources":["C:\\Users\\co